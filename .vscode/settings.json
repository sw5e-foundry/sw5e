{
  "editor.formatOnSave": true,
  "less.compile": {
    "main": [
      "${workspaceRoot}\\less\\original\\sw5e.less",
      "${workspaceRoot}\\less\\update\\sw5e-global.less",
      "${workspaceRoot}\\less\\update\\sw5e-light.less",
      "${workspaceRoot}\\less\\update\\sw5e-dark.less"
    ],
    "out": "${workspaceRoot}\\"
  },
  "eslint.format.enable": true,
  "editor.defaultFormatter": null,
  "editor.formatOnPaste": true,
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "hbs"
  ],
  "json.format.enable": false,
  "html.format.enable": false
}
