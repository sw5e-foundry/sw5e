{"_id":"Iu1wYYanefSvWwGB","name":"Mesmer's Routine","type":"classfeature","img":"systems/sw5e/packs/Icons/Class%20Features/SCT-Passive.webp","effects":[],"folder":null,"sort":0,"flags":{"timestamp":"2022-10-18T11:21:01.0729588+00:00","importer_version":2,"uid":"Feature.name-mesmers_routine.source-classinvocation.sourceName-scout.level-None"},"system":{"description":{"value":"<p>At the start of each of your turns, you can choose to have advantage on saving throws against effects that would incapacitate you or put you to sleep until the start of your next turn. Alternatively, you can choose to extend this benefit to allies within 5 feet of you.</p>","chat":"","unidentified":""},"requirements":"Scout","source":"PHB","activation":{"type":null,"cost":null,"condition":""},"duration":{"value":null,"units":"inst"},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":"5","long":null,"units":"","unit":"feet"},"uses":{"value":null,"max":null,"per":null},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"other","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"power"},"recharge":{"charged":false,"value":null},"className":null},"ownership":{"default":0,"vorSwUmgv9YMdSPD":3},"_stats":{"systemId":"sw5e","systemVersion":"2.0.2.2.2.4","coreVersion":"10.288","createdTime":null,"modifiedTime":1667426105081,"lastModifiedBy":"MO2mq4AAHmz86wzy"}}
